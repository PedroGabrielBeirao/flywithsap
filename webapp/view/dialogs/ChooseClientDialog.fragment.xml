<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found"
		title="Select Product"
		liveChange=".onSearch"
		confirm=".onValueHelpDialogClose"
		cancel=".onValueHelpDialogClose"
		items="{
			path: '/ClienteSet',
			sorter: {
				path: 'Nome'
			}
		}">
		<StandardListItem
			selected="false"
			title="{Nome}"
			description="{Codigo}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>