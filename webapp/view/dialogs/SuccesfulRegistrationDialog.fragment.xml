<core:FragmentDefinition 
	xmlns:core="sap.ui.core" 
	xmlns="sap.m">
	<Dialog 
		icon="sap-icon://hr-approval" 
		showHeader="true" 
		title="{i18n>showHeader}"
		type="Standard">
		<content>
			<Text text="{i18n>userCreated}" 
				  width="296px" 
				  maxLines="1" 
				  wrapping="false" 
				  textAlign="Center" 
				  textDirection="Inherit"
				  visible="true"/>
		</content>
		<buttons>
			<Button text="{i18n>buttonContinue}" 
					type="Default" 
					icon="sap-icon://flight" 
					iconFirst="true" 
					width="134px" 
					enabled="true" 
					visible="true"
					iconDensityAware="false" 
					press="onNavBack"/>
			<Button text="{i18n>buttonSearchAgain}" 
					type="Default" 
					icon="sap-icon://refresh" 
					iconFirst="true" 
					width="auto" 
					enabled="true" 
					visible="true"
					iconDensityAware="false" 
					press="onNavToHome"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>